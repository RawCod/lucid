<div class="">
  <h1>My bookings:</h1>
  <h3><%= link_to 'Back to home', root_path %></h3>


  <div class="booking-card">
    <% @bookings.each do |booking| %>
      <div class = "new-event" data-aos="zoom-in-up" data-aos-duration=90>
        <p><strong>Name: </strong><%=booking.event.name%></p>
        <p><strong>Location: </strong><%=booking.event.location%></p>
        <p><strong>Capacity: </strong><%=booking.event.capacity%></p>
        <p><strong>Description: </strong><%=booking.event.description%></p>
        <p><strong>Category: </strong><%=booking.event.category%></p>
        <%# <%= link_to 'Edit', edit_booking_path(booking)  %>
        <%# <%= link_to 'Delete', booking_path(booking), method: :delete, data: { confirm: "Are you sure?" } %>
      </div>
    <% end %>
  </div>
</div>
